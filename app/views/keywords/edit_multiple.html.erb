<%= form_for :keyword, url: {action: 'update_multiple'} do -%>
  
  
    <% @keywords.each_with_index do |keyword, index| %>
      <%= fields_for "keyword[]", keyword do |f| %>
    
        <div class="field">
            <label>Palabra Clave <%= index + 1 %></label><br>
            <%= f.text_field :keyword %><br>
       
        </div>
     
      <% end %>
    <% end %>
  
   
    
   
    
     
    
    
      
    
    
    
  
    <%= submit_tag 'Update' %>
<% end %>